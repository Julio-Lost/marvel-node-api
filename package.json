{
  "name": "stone-challenge-api",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/Julio-Lost/stone-challenge-api.git",
  "author": "Julio-Lost <juliolost00@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "test:staged": "jest",
    "style:check": "prettier --check src/**/*.ts",
    "style:fix": "prettier --write src/**/*.ts",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "typeorm:dev": "NODE_ENV=development ts-node ./node_modules/typeorm/cli.js --config cli.typeorm.development.js",
    "typeorm": "NODE_ENV=production ts-node ./node_modules/typeorm/cli.js --config cli.typeorm.production.js",
    "prod": "NODE_ENV=production node dist/index.js",
    "run-migration": "yarn typeorm:dev schema:sync",
    "dev": "NODE_ENV=development ts-node-dev -r tsconfig-paths/register src/index.ts"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.31",
    "@types/node-fetch": "^2.5.8",
    "eslint": "^7.20.0",
    "husky": "^5.1.1",
    "i": "^0.3.6",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "standard": "^16.0.3",
    "ts-jest": "^26.5.2",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^2.6.1",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.2",
    "typeorm": "^0.2.31"
  }
}
